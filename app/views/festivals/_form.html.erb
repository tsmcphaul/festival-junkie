<%= form_for @festival do |f|%>

<%= f.label :title %>
<%= f.text_field :title%>

<%= f.label :date %>
<%= f.date_select :date%>

<%= f.label :city %>
<%= f.text_field :city%>

<%= f.label :state %>
<%= select_tag :state, options_for_select(us_states) %>

<%= f.label :venue %>
<%= f.text_field :venue%>

<%= f.label :genre %>
<%= select_tag :genre, options_for_select(genres)%>

<%= f.label :artist %>
<%= f.text_field :artist%>

<%= f.submit "Add Festival"%>

<% end %>
